$(function(){
	//Description	:	When select equipment type gets changed, focus on the serial number for scanning
	//				:	aleviates the need for the field technician/operator to manually select the serial_number
	//Programmer	:	Matt Chung
	//Date			:	2/22/2012
	
	
	$("select[name=eq_type_id]").bind('change',function(){
		var txtbox_serial_number	=	$('input[name=eq_serial]');
		txtbox_serial_number.focus();
	})
	
	
})
